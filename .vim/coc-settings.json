{
  "coc.preferences.messageLevel": "warning",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "go",
    "dart",
    "haskell",
    "html",
    "lua",
    "javascript",
    "javascriptreact",
    "json",
    "markdown",
    "python",
    "rust",
    "typescript",
    "typescriptreact",
    "yaml"
  ],
  "languageserver": {
    "dart": {
      "command": "dart_language_server",
      "args": [],
      "filetypes": ["dart"],
      "initializationOptions": {},
      "settings": {
        "dart": {
          "validation": {},
          "completion": {}
        }
      }
    },
    "dockerfile": {
      "command": "docker-langserver",
      "args": ["--stdio"],
      "filetypes": ["dockerfile"]
    },
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["vim", "markdown"]
    },
    "golang": {
      "command": "gopls",
      "filetypes": ["go"],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"]
    },
    "haskell": {
      "command": "hie-wrapper",
      "initializationOptions": {},
      "filetypes": ["hs", "lhs", "haskell"],
      "rootPatterns": [".stack.yaml", "cabal.config", "package.yaml"],
      "settings": {
        "languageServerHaskell": {
          "hlintOn": false,
          "maxNumberOfProblems": 10,
          "completionSnippetsOn": true
        }
      }
    },
    "lua": {
      "command": "lua-lsp",
      "filetypes": ["lua"]
    },
    "python": {
      "command": "python",
      "args": [
        "-mpyls",
        "-vv",
        "--log-file",
        "/tmp/lsp_python.log"
      ],
      "trace.server": "verbose",
      "filetypes": ["python"],
      "settings": {
        "pyls": {
          "enable": true,
          "trace": {
            "server": "verbose"
          },
          "commandPath": "",
          "configurationSources": [
            "pycodestyle"
          ],
          "plugins": {
            "jedi_completion": {
              "enabled": true
            },
            "jedi_hover": {
              "enabled": true
            },
            "jedi_references": {
              "enabled": true
            },
            "jedi_signature_help": {
              "enabled": true
            },
            "jedi_symbols": {
              "enabled": true,
              "all_scopes": true
            },
            "mccabe": {
              "enabled": true,
              "threshold": 15
            },
            "preload": {
              "enabled": true
            },
            "pycodestyle": {
              "enabled": true
            },
            "pydocstyle": {
              "enabled": false,
              "match": "(?!test_).*\\.py",
              "matchDir": "[^\\.].*"
            },
            "pyflakes": {
              "enabled": true
            },
            "rope_completion": {
              "enabled": true
            },
            "yapf": {
              "enabled": true
            }
          }
        }
      }
    },
    "rust": {
      "command": "ra_lsp_server",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"]
    }
  },
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.nodePath": ".vscode/pnpify",
  "eslint.codeAction.disableRuleComment": {
    "enable": true,
    "location": "sameLine"
  },
  "git.addGlameToVirtualText": true,
  "git.addGlameToBufferVar": true,
  "html.enable": true,
  "html.format.enable": true,
  "html.validate.html": true,
  "html.validate.scripts": true,
  "html.validate.styles": true,
  "json.enable": true,
  "json.format.enable": true,
  "lists.disabledLists": [],
  "list.source.grep.command": "rg",
  "prettier.arrowParens": "always",
  "prettier.bracketSpacing": true,
  "prettier.eslintIntegration": false,
  "prettier.jsxBracketSameLine": false,
  "prettier.jsxSingleQuote": false,
  "prettier.printWidth": 80,
  "prettier.proseWrap": "always",
  "prettier.requireConfig": false,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.statusItemText": "prettier",
  "prettier.stylelintIntegration": false,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "all",
  "prettier.useTabs": false,
  "rust-client.updateOnStartup": false,
  "stylelint.enable": true,
  "tsserver.enable": true,
  "tsserver.enableJavascript": true,
  "tsserver.formatOnType": true,
  "tsserver.npm": "/home/meatwallace/.asdf/shims/npm",
  "tsserver.reportStyleChecksAsWarnings": true,
  "tsserver.tsdk": ".vscode/pnpify/typescript/lib",
  "tsserver.disableAutomaticTypeAcquisition": false,
  "typescript.implementationsCodeLens.enable": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.noSemicolons": false,
  "typescript.referencesCodeLens.enable": true,
  "typescript.suggestionActions.enabled": true,
  "typescript.suggest.enabled": true,
  "typescript.suggest.paths": true,
  "typescript.suggest.autoImports": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.showUnused": true,
  "typescript.updateImportsOnFileMove.enable": false,
  "typescript.validate.enable": true,
  "yaml.format.enable": true,
  "yaml.format.singleQuote": true,
  "yaml.format.bracketSpacing": false,
  "yaml.format.proseWrap": "always",
  "yaml.validate": true,
  "yaml.hover": true,
  "yaml.completion": true,
  "yaml.schemas": {
    "http://json.schemastore.org/circleciconfig": "/.circleci/config.yml",
    "http://json.schemastore.org/huskyrc": "/.huskyrc.yml",
    "http://json.schemastore.org/travis": "/.travis.yml",
    "http://json.schemastore.org/stylelintrc": "/.stylelintrc.yml",
    "http://json.schemastore.org/eslintrc": "/.eslintrc.yml",
    "http://json.schemastore.org/gitlab-ci": "/.gitlab-ci.yml",
    "http://json.schemastore.org/prettierrc": "/.prettierrc.yml"
  },
  "yank.highlight.enable": true
}
