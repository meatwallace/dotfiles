{
  "console.log": {
    "prefix": "clg",
    "body": ["console.log('$1');", "$0"],
    "description": "Log output to the console"
  },
  "constant": {
    "prefix": "econ",
    "body": ["export const $1 = $2;", "$0"],
    "description": "Create a named, exported constant"
  },
  "react-functional-component": {
    "prefix": "rfc",
    "body": [
      "import React from 'react';",
      "import { ${3:propTypes} } from 'prop-types';",
      "",
      "const ${1:ComponentName} = (props) => {",
      "\tconst { ${2:props} ...rest } = props;",
      "",
      "\treturn (",
      "\t\t$0",
      "\t);",
      "}",
      "",
      "${1:ComponentName}.propTypes = {",
      "\t${2:props}",
      "};",
      "",
      "export default ${1:ComponentName};"
    ],
    "description": "React functional component"
  }
}
