{
  ":: Theme": "",
  "editor.fontFamily": "Operator Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.lineHeight": 20,
  "material-icon-theme.folders.theme": "specific",
  "workbench.colorTheme": "Night Owl",
  "workbench.iconTheme": "material-icon-theme",

  ":: Settings": "",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.detectIndentation": false,
  "editor.folding": false,
  "editor.formatOnSave": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.insertSpaces": true,
  "editor.matchBrackets": true,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.parameterHints": false,
  "editor.quickSuggestions": false,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "none",
  "editor.scrollBeyondLastLine": false,
  "editor.snippetSuggestions": "none",
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "files.defaultLanguage": "javascript",
  "files.useExperimentalFileWatcher": true,
  "keyboard.touchbar.enabled": false,
  "window.zoomLevel": 1,
  "workbench.activityBar.visible": false,
  "workbench.editor.showIcons": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.feedback.visible": false,
  "workbench.statusBar.visible": true,
  "zenMode.fullScreen": false,
  "zenMode.hideStatusBar": false,
  "zenMode.restore": true,

  ":: File assosciations": "",
  "files.associations": {
    "*.js": "javascript"
  },

  ":: Git": "",
  "git.autofetch": true,

  ":: CSS": "",
  "css.validate": false,

  ":: HTML": "",
  "html.format.enable": false,
  "html.suggest.angular1": false,
  "html.suggest.ionic": false,
  "html.validate.scripts": false,
  "html.validate.styles": false,

  ":: Javascript": "",
  "javascript.format.enable": false,
  "javascript.nameSuggestions": false,
  "javascript.validate.enable": false,
  "jsDocCompletion.enabled": false,
  "typescript.autoImportSuggestions.enabled": false,
  "typescript.quickSuggestionsForPaths": false,
  "typescript.tsc.autoDetect": "off",
  "typescript.validate.enable": false,

  ":: JSON": "",
  "json.format.enable": false,

  ":: Markdown": "",
  "markdownlint.config": {
    "MD013": {
      "line_length": 80
    },
    "MD029": {
      "style": "ordered"
    }
  },

  "::Reason": "",

  ":: Shell": "",
  "shellcheck.enable": true,
  "shellcheck.run": "onType",
  "shellcheck.executablePath": "shellcheck",

  ":: SVG": "",
  "svgviewer.enableautopreview": false,

  ":: ESLint": "",
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",

  ":: Flow": "",
  "flow.useNPMPackagedFlow": true,

  ":: Prettier": "",
  "prettier.eslintIntegration": true,
  "prettier.requireConfig": true,

  ":: Emmet": "",
  "emmet.triggerExpansionOnTab": false,

  ":: import-sort": "",
  "sort-imports.default-sort-style": "module-compact",
  "sort-imports.supress-warnings": true,

  ":: TODOs": "",
  "todohighlight.keywords": ["TODO", "DOC", "DEPRECATE"],
  "todohighlight.defaultStyle": {
    "backgroundColor": "#45474F",
    "color": "#CBCDD2"
  },

  ":: GitLens": "",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.historyExplorer.enabled": true,
  "gitlens.mode.active": "zen"
}
